{
  "name": "megaapi-md",
  "version": "4.0.0",
  "description": "API MultiDevice - MegaApi",
  "engines": {
    "node": "16.18.0",
    "npm": "8.19.2"
  },
  "scripts": {
    "build": "npm run tsc --declaration --emitDeclarationOnly",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "start:dev": "cross-env NODE_ENV=development node dist/app.js",
    "test": "pm2 -f start src/app.js --name 11111 --namespace API_TESTE -- 11111 teste https://webhook.site/",
    "pedro": "pm2 -sf start src/app.js --watch ./src --name 11111 --namespace API_TESTE -- 11111 teste https://webhook.site//"
  },
  "keywords": [],
  "author": "barkerpedro",
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/baileys": "^4.4.0",
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^10.0.0",
    "axios": "^1.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.0",
    "https": "^1.0.0",
    "link-preview-js": "^2.1.19",
    "method-override": "^3.0.0",
    "mime-types": "^2.1.35",
    "path": "^0.12.7",
    "pino": "^8.7.0",
    "pm2": "^5.2.2",
    "qrcode": "^1.5.1",
    "qrcode-terminal": "^0.12.0",
    "requestretry": "^7.0.2",
    "sharp": "^0.30.7",
    "shelljs": "^0.8.5",
    "ts-log-debug": "^5.5.3",
    "youtube-thumbnail": "^1.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/method-override": "^0.0.32",
    "@types/node": "^18.11.3",
    "@types/pino": "^7.0.5",
    "@types/qrcode": "^1.4.1",
    "@types/requestretry": "^1.12.8",
    "@types/shelljs": "^0.8.11",
    "@types/ws": "^8.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}